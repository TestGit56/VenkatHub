<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
<class name = "com.org.dao.Department" table = "DEPARTMENT">

<id name = "deptId" column = "DEPT_ID">
<generator class = "assigned"></generator>
</id>

<property name = "deptName" column = "DEPT_NAME"></property>

<set name = "employeeSet" table = "EMPLOYEE" inverse="false" lazy="false" cascade="all">
<key column = "DEPT_FK_ID"></key>
<one-to-many class = "com.org.dao.Employee"/>
</set>
</class>

</hibernate-mapping>